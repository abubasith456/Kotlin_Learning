<?xml version="1.0" encoding="utf-8"?>
<layout>
    
    <data>
        <variable
            name="offlineNewsViewModel"
            type="com.example.kotlinlearning.viewModel.OfflineNewsViewModel" />
    </data>


    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragment.OfflineNewsFragment">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewOfflineNews"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:listitem="@layout/list_view_items" />

    </FrameLayout>
</layout>